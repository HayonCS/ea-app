{
  "name": "ea-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "child_process": false
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "execute": "cross-env NODE_ENV=production yarn build:server && cross-env PORT=3001 concurrently \"yarn dev:agent\" \"yarn server:local\"",
    "initial": "yarn redis:load:assets && yarn redis:load:employees",
    "build:jsonschema": "node scripts/json-schema-types.js",
    "build:graphql": "gql-gen -o",
    "build:client": "webpack --config ./webpack/client.config.js",
    "build:server": "webpack --config ./webpack/server.config.js",
    "build:pre": "concurrently \"yarn build:jsonschema\" \"yarn build:graphql\"",
    "build:app": "concurrently \"yarn build:client\" \"yarn build:server\"",
    "build": "yarn build:pre && yarn build:app",
    "build:local": "cross-env NODE_ENV=production AUTH_LOCAL=true yarn build",
    "build:production": "cross-env NODE_ENV=production AUTH_LOCAL=false yarn build",
    "server:local": "cross-env AUTH_LOCAL=true node dist/server.js",
    "server:production": "cross-env AUTH_LOCAL=false PORT=3001 node dist/server.js",
    "dev:jsonschema": "nodemon -w \"./modules\" -e .schema.json ./scripts/json-schema-types.js",
    "dev:graphql": "nodemon -w \"./modules\" -e .graphql --exec yarn \"build:graphql\"",
    "dev:server:webpack": "webpack --config webpack/server.config.js --watch",
    "dev:server:nodemon": "nodemon --watch dist/server.js --max-old-space-size=16000 --inspect=9229 dist/server.js",
    "dev:worker:nodemon": "nodemon --watch 'dist/scripts/{job-worker.js,*processor.js}' -- --inspect=9230 dist/scripts/job-worker.js",
    "dev:client": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack-dev-server --config webpack/client.config.js",
    "dev:stop": "utils/kill-server",
    "dev:log": "DEBUG=knex:query,knex:bindings,save,db yarn dev 2>&1 | stdbuf -o0 sed -E \"s/.\\[[0-9]{0,2}m//g\" | tee ~/app/dev.log",
    "dev": "concurrently -p name -c \"white,yellow,magenta,green,red,gray,blue\" -n \"webpack-server,nodemon-server,webpack-client,graphql-types,json-schema,workers\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\" \"yarn dev:client\" \"yarn dev:graphql\" \"yarn dev:jsonschema\" \"yarn dev:worker:nodemon\"",
    "dev:local": "yarn build:local && yarn server:local",
    "dev:production": "yarn build:production && yarn server:production",
    "dev:storybook": "cross-env STORYBOOK_MODE=true App_ENDPOINT=prod start-storybook -s ./ -p 9001 -c .storybook --ci",
    "redis:load:assets": "sudo NODE_ENV=development node ./dist/scripts/load-asset-list-bi.js",
    "redis:load:employees": "sudo NODE_ENV=development node ./dist/scripts/load-employee-directory.js",
    "redis:load:processdata": "sudo NODE_ENV=development node ./dist/scripts/load-process-data.js"
  },
